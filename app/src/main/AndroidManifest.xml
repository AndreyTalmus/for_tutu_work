<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.homeproduction.andrey.tututest">

    <uses-sdk android:minSdkVersion="14" />

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>gi
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListActivity"
            android:label="Выберите станцию"
            android:screenOrientation="portrait" />
        <activity android:name=".StationActivity"
            android:label="Информация о станции"
            android:screenOrientation="portrait" />
        <activity android:name=".AboutApplication"
            android:label="Информация:"
            android:screenOrientation="portrait" />
    </application>

</manifest>